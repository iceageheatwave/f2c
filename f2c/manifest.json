{
    "name": "F2C Converter",
    "version": "1.12",
    "manifest_version": 2,
    "description": "Simple app to convert temperatures from fahrenheit to celcius.",
    "content_scripts": [
    {
        "matches": ["http://*/*", "file://*"],
